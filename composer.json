{
    "name": "mvaliolahi/attachable",
    "description": "This package allows you to attach files to Eloquent models.",
    "authors": [
        {
            "name": "Meysam Valiolahi",
            "email": "mvaliolahi@gmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mvaliolahi\\Attachable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "intervention/image": "^2.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mvaliolahi\\Attachable\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.5",
        "laravel/framework": "^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
